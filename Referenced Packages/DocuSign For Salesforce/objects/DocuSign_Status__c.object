    <fields>
        <fullName>Time_to_Complete__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( Envelope_Status__c  = &quot;Voided&quot;, &quot;&quot;,  IF(Envelope_Status__c  = &quot;Declined&quot;,  &quot;&quot;,   IF(Envelope_Status__c  = &quot;Sent&quot;,  &quot;&quot;, TEXT( 
ROUND( 
( Completed_Date_Time__c  -  Sent_Date_Time__c ) - 
MOD(Completed_Date_Time__c  - Sent_Date_Time__c,1) 
,0) 
) 
&amp;&quot; day(s), &quot;&amp; 
TEXT( 
ROUND( 
MOD(Completed_Date_Time__c - Sent_Date_Time__c,1)*24 - 
MOD(MOD(Completed_Date_Time__c - Sent_Date_Time__c,1)*24,1) 
,0) 
) 
&amp;&quot; hour(s), &quot;&amp; 
TEXT( 
ROUND( 
MOD(MOD(Completed_Date_Time__c - Sent_Date_Time__c,1)*24,1) 
*60 
,0) 
)&amp;&quot; minute(s)&quot;) ) )</formula>
        <label>Time to Complete</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
